<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
<!-->add_test</!-->
  <servlet>
    <servlet-name>UserRestControllerV1</servlet-name>
    <servlet-class>servlet1.UserRestControllerV1</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>UserRestControllerV1</servlet-name>
    <url-pattern>/User</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>index</servlet-name>
    <servlet-class>index.jsp</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>EventRestControllerV1</servlet-name>
    <servlet-class>servlet1.EventRestControllerV1</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>EventRestControllerV1</servlet-name>
    <url-pattern>/Event</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>FileRestControllerV1</servlet-name>
    <servlet-class>servlet1.FileRestControllerV1</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>FileRestControllerV1</servlet-name>
    <url-pattern>/File</url-pattern>
  </servlet-mapping>

</web-app>
